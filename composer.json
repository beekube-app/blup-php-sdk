{
    "name": "beekube-app/blup-php-sdk",
    "version": "1.0.3",
    "description": "SDK PHP pour l'API BLUP de Beekube - Calcul génétique des abeilles mellifères",
    "keywords": [
        "beekube",
        "blup",
        "mellifera",
        "bee",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://www.beekube.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Beekube",
            "homepage": "https://beekube.com"
        },
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4.5",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "overtrue/phplint": "^9.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": { "Beekube\\BlupSDK\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Beekube\\BlupSDK\\Test\\" : "tests/" }
    },
    "scripts": {
        "test": [
            "@phplint"
        ],
        "phplint": "phplint"
    }
}
